{
    "name": "kptv-proxy-admin",
    "version": "1.0.0",
    "description": "KPTV Proxy Admin Interface",
    "scripts": {
        "build:css": "tailwindcss -i ./static/admin-source.css -o ./static/admin.css --minify",
